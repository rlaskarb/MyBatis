<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 매퍼 이름 등록하기 -->
<mapper namespace="mapper">
    <select id="selectSysDate" resultType="java.util.Date">
        SELECT
            CURDATE()
        FROM
            DUAL
    </select>
</mapper>